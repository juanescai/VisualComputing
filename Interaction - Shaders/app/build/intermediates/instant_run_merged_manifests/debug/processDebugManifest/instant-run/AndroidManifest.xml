<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="processing.test.toonshading"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="28" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="Universe"
        android:testOnly="true" >
        <activity
            android:name="processing.test.toonshading.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.0.0" />

        <uses-library
            android:name="com.google.android.wearable"
            android:required="false" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="processing.test.toonshading.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>